{% extends '::base.html.twig' %}

{% block body -%}
    <h1>VideoPrize</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Prizeamount</th>
                <td>{{ entity.prizeAmount }}</td>
            </tr>
            <tr>
                <th>Prizeplace</th>
                <td>{{ entity.prizePlace }}</td>
            </tr>
            <tr>
                <th>Createdate</th>
                <td>{{ entity.createDate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('premiacao_video') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('premiacao_video_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
